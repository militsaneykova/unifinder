<% include ../partials/boilerplate %>

<h1>Welcome to notes-index page </h1>
  <% data.forEach(note => { %>
      <p><%= note.description %></p>
        <% if (note.completed) { %>
    <span class='done'>Done!</span>
            <% } else { %>
    <span class='not-done' data-id='<%= note.id %>'>Not done!</span>
  <% } %>
  <a href='/auth/notes/<%= note.id %>'>Link</a>
<% }) %>
<script src='/src/main.js'></script>

<% include ../partials/end %>